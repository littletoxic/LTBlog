{
  "Logging": {
    "LogLevel": {
      "Default": "Trace"
      //"Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "api.littletoxic.top;littletoxic.top",
  "ReverseProxy": {
    "Routes": {
      "api": {
        "ClusterId": "api",
        "Match": {
          "Hosts": [ "api.littletoxic.top" ],
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "api": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5281"
          }
        }
      }
    }
  }
}
